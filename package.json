  "scripts": {
    "start": "nodemon --watch server/data/**/*.js --exec 'npm run update && npm run start-dev-server'",
    "start-dev-server": "sh scripts/run-server.sh",
    "update": "./node_modules/.bin/babel-node server/scripts/update-schema.js && npm run relay",
    "deploy": "npm run clean && cross-env NODE_ENV=production npm run build-client && npm run update && npm run build-server && node ./build/server/index.js",
    "build": "npm run build-server && npm run build-client",
    "build-client": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build-server": "sh scripts/build-server.sh",
    "build-server-dev": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "docker:clean-images": "sh scripts/docker-clean-images.sh",
    "docker:build": "sh scripts/docker-build.sh",
    "docker:run": "sh scripts/docker-run.sh",
    "docker:run:full": "sh scripts/docker-run-full.sh",
    "docker:login": "sh scripts/docker-login-aws.sh",
    "docker:push": "sh scripts/docker-push-aws.sh",
    "docker:deploy": "npm run docker:login && npm run docker:push",
    "docker:deploy:full": "npm run docker:build && npm run docker:login && npm run docker:push",
    "flow": "flow",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:fix": "npm run lint:fix:client && npm run lint:fix:server",
    "lint:client": "eslint client",
    "lint:server": "eslint server",
    "lint:fix:client": "eslint --fix client",
    "lint:fix:server": "eslint --fix server",
    "clean": "sh scripts/clean-build.sh",
    "relay": "sh scripts/relay-compiler.sh"
  },
